-- rectangle structure
load system io.
load system type.

structure Rectangle with
  data xdim.
  data ydim.

  -- member function
  function area with () do
    return this@xdim * this@ydim.
  end
end

let r = Rectangle(4,2).
io @println ("The area of Rectangle ("+r@xdim+","+r@ydim+") is "+r@area()).
